{
   "name": "express-typescript-starter",
   "version": "0.1.0",
   "description": "A starting point for Node.js express apps with TypeScript",
   "repository": {
      "type": "git",
      "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
   },
   "engines": {
      "node": "9.5.0"
   },
   "author": "Bowden Kelly",
   "license": "MIT",
   "scripts": {
      "start": "npm run serve",
      "apidocs": "apidoc -i ./src/controllers -o ./apidocs/",
      "build": "npm run build-ts && npm run tslint",
      "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
      "serve": "node dist/server.js",
      "watch-node": "nodemon dist/server.js",
      "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
      "test": "jest --forceExit --coverage --verbose",
      "watch-test": "npm run test -- --watchAll",
      "build-ts": "tsc",
      "watch-ts": "tsc -w",
      "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
      "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
      "tslint": "tslint -c tslint.json -p tsconfig.json",
      "copy-static-assets": "ts-node copyStaticAssets.ts",
      "debug": "npm run build && npm run watch-debug",
      "serve-debug": "nodemon --inspect dist/server.js",
      "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
      "heroku-postbuild": "npm run build && cd client && npm install && npm run build"
   },
   "dependencies": {
      "@types/express-validator": "^3.0.0",
      "@types/jsonwebtoken": "^7.2.7",
      "@types/passport-jwt": "^3.0.1",
      "@types/web-push": "^3.3.0",
      "apidoc": "^0.17.7",
      "async": "^2.6.0",
      "bcrypt-nodejs": "^0.0.3",
      "bluebird": "^3.5.1",
      "body-parser": "^1.18.2",
      "chalk": "^2.4.1",
      "compression": "^1.7.1",
      "continuation-local-storage": "^3.2.1",
      "cors": "^2.8.4",
      "dotenv": "^4.0.0",
      "errorhandler": "^1.5.0",
      "express": "^4.16.2",
      "express-validator": "^4.3.0",
      "jsonwebtoken": "^8.2.1",
      "lodash": "^4.17.11",
      "lusca": "^1.5.2",
      "morgan": "^1.9.1",
      "nodemailer": "^4.4.1",
      "passport": "^0.4.0",
      "passport-google-oauth": "^1.0.0",
      "passport-google-oauth2": "^0.1.6",
      "passport-jwt": "^4.0.0",
      "passport-local": "^1.0.0",
      "pg": "^7.3.0",
      "pug": "^2.0.4",
      "reflect-metadata": "^0.1.12",
      "request": "^2.88.0",
      "request-promise": "^4.2.2",
      "sequelize": "^4.41.0",
      "sequelize-cli": "^5.5.0",
      "sequelize-typescript": "^0.6.11",
      "strip-ansi": "^4.0.0",
      "uuid": "^3.3.2",
      "web-push": "^3.3.3",
      "winston": "^2.4.0"
   },
   "devDependencies": {
      "@fortawesome/fontawesome-free": "^5.9.0",
      "@types/async": "^2.0.45",
      "@types/bcrypt-nodejs": "^0.0.30",
      "@types/bluebird": "^3.5.20",
      "@types/body-parser": "^1.16.8",
      "@types/compression": "^0.0.35",
      "@types/cors": "^2.8.4",
      "@types/dotenv": "^4.0.2",
      "@types/errorhandler": "^0.0.32",
      "@types/express": "latest",
      "@types/jest": "^22.1.3",
      "@types/jquery": "^3.2.17",
      "@types/lodash": "^4.14.91",
      "@types/lusca": "^1.5.0",
      "@types/morgan": "^1.7.35",
      "@types/node": "^8.0.29",
      "@types/nodemailer": "^4.3.4",
      "@types/passport": "^0.4.3",
      "@types/passport-google-oauth2": "^0.1.2",
      "@types/passport-local": "^1.0.32",
      "@types/request": "^2.47.0",
      "@types/shelljs": "^0.7.8",
      "@types/supertest": "^2.0.4",
      "@types/winston": "^2.3.7",
      "chai": "^4.1.2",
      "concurrently": "^3.5.1",
      "jest": "^22.0.4",
      "node-sass": "^4.12.0",
      "nodemon": "^1.19.1",
      "shelljs": "^0.8.1",
      "supertest": "^3.0.0",
      "ts-jest": "^22.0.4",
      "ts-node": "3.3.0",
      "tslint": "^5.9.1",
      "typescript": "2.9"
   }
}
